{% extends '::base.html.twig' %}

{% block stylesheets %}
{{ parent() }}
<link href="{{ asset("http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css") }}" type="text/css" rel="stylesheet" />
<link href="{{ asset('bundles/monubitmap/css/map.css') }}" type="text/css" rel="stylesheet" />
{% endblock %}
{% block javascripts %}
{{ parent() }}
<script type="text/javascript" src="{{ asset("http://code.jquery.com/ui/1.10.3/jquery-ui.js") }}"></script>

{% endblock %}

{% block body %}
{% spaceless %}

<div class="content" >
	{% if map is defined %}
		{{ google_map_container(map) }}
		{{ google_map_js(map) }}
		{{ google_map_css(map) }}
	{%  endif %}
</div>

{% endspaceless %}
{% endblock %}